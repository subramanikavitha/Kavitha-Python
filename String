b = int(input('Enter second number: '))
c = int(input('Enter third number: '))

d = int(input('Enter fourth number: '))
e = int(input('Enter fifth number: '))
n= [a, b, c, d, e]
for num in n:
 if num%2 == 0:
  print(num, 'is an even number')
  continue
 else:
   print(num,"is odd number ")



